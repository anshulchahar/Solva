{
  "name": "docanalyze",
  "version": "1.0.0",
  "description": "A retro-themed PDF document analysis tool powered by Google's Gemini AI.",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint static/js/*.js",
    "lint:fix": "eslint --fix static/js/*.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build:css": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/css/style.css",
    "watch:css": "npx tailwindcss -i ./static/css/tailwind.css -o ./static/css/style.css --watch",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "jwt-decode": "^3.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/mocks/styleMock.js"
    }
  }
}